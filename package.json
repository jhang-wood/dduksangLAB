{
  "name": "dduksang-lab",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:silent": "NODE_ENV=production next build > /dev/null 2>&1 && echo '✅ Build completed successfully' || echo '❌ Build failed'",
    "start": "next start",
    "lint": "next lint",
    "env:validate": "node scripts/validate-env.js",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test:ci && npm run test:e2e",
    "claude": "node scripts/claude-helper.js",
    "claude:fix": "node scripts/claude-helper.js fix",
    "claude:commit": "node scripts/claude-helper.js commit",
    "claude:deploy": "node scripts/claude-helper.js deploy",
    "claude:deploy-full": "node scripts/claude-helper.js deploy --build",
    "claude:rollback": "node scripts/claude-helper.js rollback",
    "claude:status": "node scripts/claude-helper.js status",
    "prepare": "husky install",
    "pre-commit": "npm run type-check && npm run test:ci"
  },
  "dependencies": {
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.38.0",
    "bullmq": "^5.56.10",
    "dotenv": "^17.2.0",
    "framer-motion": "^10.16.4",
    "ioredis": "^5.7.0",
    "lucide-react": "^0.292.0",
    "next": "^14.2.31",
    "node-cron": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.48.0",
    "eslint-config-next": "14.0.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.31",
    "resize-observer-polyfill": "^1.5.1",
    "string-replace-loader": "^3.2.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.0",
    "whatwg-fetch": "^3.6.20"
  }
}
